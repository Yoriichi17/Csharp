using System;
class P3
{
    static void Main()
    {
        Console.WriteLine("Enter the string : ");
        string s = Console.ReadLine();
        int len = s.Length;
        for(int i = 0; i < len; i++)
        {
            for(int j = 1 ; j<=len-i; j++)
            {
                string t = s.Substring(i, j);
                Console.WriteLine(t);
            }
        }
    }
}
